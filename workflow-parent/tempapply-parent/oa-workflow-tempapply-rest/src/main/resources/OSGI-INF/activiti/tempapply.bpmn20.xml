            </extensionElements>
        </userTask>
        <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow3" sourceRef="departmentHead" targetRef="exclusivegateway1"></sequenceFlow>
